{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for exprss api service with usually libs setting",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node --presets es2015,stage-0 ./src/starter/init_dev.js",
    "prestart": "babel src --presets es2015,stage-0 --ignore node_modules,init_dev.js --out-dir dist",
    "start": "node ./dist/starter/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WJWang/express-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WJWang/express-boilerplate/issues"
  },
  "homepage": "https://github.com/WJWang/express-boilerplate#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "morgan": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.2"
  }
}
